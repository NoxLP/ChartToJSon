<UserControl x:Class="ChartCanvasNamespace.Lines.LineDragger"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ChartCanvasNamespace.Lines"
             mc:Ignorable="d" Cursor="Hand"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Thumb x:Name="LineThumb" DragDelta="Thumb_DragDelta"/>
        <!--DragDelta="Thumb_DragDelta"-->
        <!--MouseDown="LineDraggerMouseDown" MouseUp="LineDraggerMouseUp" MouseMove="LineDraggerMouseMove"-->
    </Grid>
</UserControl>
